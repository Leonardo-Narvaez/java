package com.krakedev;

public class TestAuto {

	public static void main(String[] args) {
		Auto auto1 = new Auto();
		Auto auto2 = new Auto();
		auto1.marca = "Nissan";
		auto1.anio = 1998;
		auto1.precio = 45.000;

		auto2.marca = "Ford";
		auto2.anio = 1967;
		auto2.precio = 60.000;
		
		System.out.println("Marca auto1 "+auto1.marca);
		System.out.println("Año auto1 "+auto1.anio);
		System.out.println("Precio auto1 "+auto1.precio);
		System.out.println("XXXXXXXXXXXXXXXXXXXXXXX");
		System.out.println("Marca auto2 "+auto2.marca);
		System.out.println("Año auto2 "+auto2.anio);
		System.out.println("Precio auto2 "+auto2.precio);

	}

}
